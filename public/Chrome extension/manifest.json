{
    "manifest_version": 3,
    "name": "Zara Product Extension",
    "version": "1.0",
    "description": "An extension that shows sustainability ratings of products",
    "permissions": ["activeTab", "scripting"],
    "content_scripts": [
      {
        "matches": ["https://www.zara.com/au/*"], 
        "js": ["extension.js"],
        "css": ["extension_styles.css"]
      }
    ]
  }
  